{
  "name": "@cloud-secure/user-service",
  "version": "1.0.0",
  "description": "Cloud Secure User Service",
  "main": "dist/main.js",
  "scripts": {
    "start:local": "cross-env NODE_ENV=local nest start",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch",
    "start:stg": "cross-env NODE_ENV=stg nest start",
    "start:prod": "cross-env NODE_ENV=prod nest start",
    "build": "nest build",
    "test": "jest",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "nodemailer": "^6.9.4",
    "reflect-metadata": "^0.1.14",
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^9.0.0",
    "@nestjs/cli": "^9.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.8.10",
    "@types/nodemailer": "^6.4.7",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}